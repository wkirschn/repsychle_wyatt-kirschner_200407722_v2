<h1>{{title}}</h1>
<p>{{title}}</p>
<a href="/items/add"> Add a New Item</a>
<table class="table table-striped table-hover">
    <tr>
        <th>Name</th>
        <th>Image</th>
        <th>EcoScore</th>

    </tr>


{{#each items}}
    <tr>
        <td>{{this.name}}</td>
        <td><img src="{{this.image}}" height="25%" width="25%"></td>
        <td>{{this.ecoScore}}</td>
    </tr>

{{/each}}

</table>